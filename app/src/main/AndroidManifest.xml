<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="swle.xyz.austers">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.GET_TASKS" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.REQUEST_IGNORE_BATTERY_OPTIMIZATIONS" />
    <uses-permission
        android:name="android.permission.INSTALL_PACKAGES"
        tools:ignore="ProtectedPermissions" />

    <application
        android:allowBackup="true"
        android:fullBackupContent="@xml/backup_descriptor"
        android:icon="@drawable/austers"
        android:label="@string/app_name"
        android:roundIcon="@drawable/austers"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".activity.IssueLostActivity"></activity>
        <activity android:name=".activity.LoseAndFindActivity" />
        <activity android:name=".activity.ClassScheduleActivity" />
        <activity android:name=".activity.VirusActivity" />
        <activity android:name=".activity.CarPoolActivity" />
        <activity android:name=".activity.ContactWayActivity">
            <intent-filter>
                <action android:name="android.intent.action.SEARCH" />
            </intent-filter>

            <meta-data
                android:name="android.app.searchable"
                android:resource="@xml/searchable" />
        </activity>
        <activity android:name=".activity.SetPwActivity" />
        <activity android:name=".activity.SignInActivity" />
        <activity android:name=".activity.MainActivity" />
        <activity android:name=".activity.PersonalInfoActivity" />
        <activity android:name=".activity.AcountBoundActivity" />
        <activity android:name=".activity.MyReleseActivity" />
        <activity android:name=".activity.MyDynamicActivity" />
        <activity android:name=".activity.MyTripActivity" />
        <activity android:name=".activity.OSLicensesActivity" />
        <activity android:name=".activity.AboutActivity" />
        <activity android:name=".activity.GradeActivity" />
        <activity android:name=".activity.SettingsActivity" />
        <activity android:name=".activity.NewsActivity" />
        <activity android:name=".activity.IdentityAuthenticateActivity" />
        <activity android:name=".activity.BannerFirstViewActivity" />
        <activity android:name=".activity.LogInActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>